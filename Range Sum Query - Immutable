class NumArray {
    List<Integer> list = new ArrayList<>();
    public NumArray(int[] nums) {
        for(int i:nums){
            list.add(i);
        }
    }
    
    public int sumRange(int i, int j) {
    	int sum = 0;
    	for(int x=i; x<=j; x++)
    	{
    		sum+=list.get(x);
    	}
    	return sum;
    }
}
